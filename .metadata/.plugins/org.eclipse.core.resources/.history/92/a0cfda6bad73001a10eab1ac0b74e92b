
public class Location {
	private int xCoord;
	private int yCoord;
	
	
	public Location(){
		this.xCoord = 0;
		this.yCoord = 0;
	}
	
	
	public Location(int xCoord, int yCoord) {
		super();
		this.xCoord = xCoord;
		this.yCoord = yCoord;
	}

	public void update(int xCoord, int yCoord) {
		this.xCoord=xCoord;
		this.yCoord = yCoord;
	}

	public int[] getCoordinates() {
		int[] c = new int[1];
		return c;
	}
}



	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
}
